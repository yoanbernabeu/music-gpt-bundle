{
    "name": "yoanbernabeu/music-gpt-bundle",
    "description": "Symfony bundle for integrating Music GPT API into your applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoan Bernabeu",
            "email": "hello@yoanbernabeu.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "symfony/config": "^7.0|^8.0",
        "symfony/dependency-injection": "^7.0|^8.0",
        "symfony/http-kernel": "^7.0|^8.0",
        "symfony/http-client": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/phpunit-bridge": "^7.0|^8.0",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^2.0",
        "symfony/var-dumper": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "YoanBernabeu\\MusicGptBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YoanBernabeu\\MusicGptBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs-fix": "vendor/bin/php-cs-fixer fix",
        "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff",
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

